{
  "name": "peer-support-lms",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/baserow/(.*)",
      "dest": "/api/baserow"
    }
  ],
  "env": {
    "BASEROW_API_URL": "@baserow_api_url",
    "BASEROW_TOKEN": "@baserow_token"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs"
}